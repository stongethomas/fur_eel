<h1>Listing members</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Role</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @members.each do |member| %>
  <tr>
    <td><%= member.name %></td>
    <td><%= member.role %></td>
	<td><%= image_tag member.photo.url(:thumb) %></td>

  </tr>
<% end %>
</table>

